<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/overlay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:background="#B3000000">
    <RelativeLayout
        android:id="@+id/typing_new_"
        android:layout_width="match_parent"
        android:layout_height="550dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:clipToOutline="true"
        android:background="#FFFFFF">
        <LinearLayout
            android:id="@+id/group"
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="156dp"
            android:baselineAligned="false">
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_1770"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_1770"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_1773"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_1773"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_1771"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_1771"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_1772"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_1772"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_353"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_353"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_296"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_296"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_1775"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_1775"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_weight="1">
                <View
                    android:id="@+id/image_307"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:background="@drawable/image_307"
                    android:layout_marginEnd="19dp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>
        </LinearLayout>
        <View
            android:id="@+id/vector_2"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="136dp"
            android:background="@drawable/vector_2"
            />
        <EditText
            android:id="@+id/eg_reply"
            android:layout_width="match_parent"
            android:background="@null"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="24dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="24dp"
            android:hint="eg : Reply Email"
            android:textColorHint="#999999"
            android:textColor="#1D3066"
            android:textSize="19dp"
            android:lineSpacingExtra="1sp"
            android:translationY="-0.45sp"
            android:gravity="top"
            android:fontFamily="@font/sfprodisplayregular"
            />
        <EditText
            android:id="@+id/description"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="56dp"
            android:hint="Description"
            android:textColorHint="#999999"
            android:textColor="#1D3066"
            android:textSize="19dp"
            android:fontFamily="@font/sfprodisplayregular"
            android:lineSpacingExtra="1sp"
            android:translationY="-0.45sp"
            android:gravity="top"
            />
        <RelativeLayout
            android:id="@+id/frame_106"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="98dp">
            <RelativeLayout
                android:id="@+id/icon_bold_s"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentRight="true">
                <View
                    android:id="@+id/send_task"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/ic_sendtask_icon"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>